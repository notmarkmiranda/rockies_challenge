<div class="container">
  <div class="list-group col-md-4 offset-md-4">
    <div class="list-group-item active">
      <h5 class="list-group-item-heading">Coaching Mound Visits By Team</h5>
    </div>
    <% @teams.each do |team| %>
    <div class="list-group-item">
      <h5 class="list-group-item-heading"><%= team.name %></h5>
      <p class="list-group-item-text"><%= team.total_mound_visits %></p>
    </div>
    <% end %>
  </div>
</div>
